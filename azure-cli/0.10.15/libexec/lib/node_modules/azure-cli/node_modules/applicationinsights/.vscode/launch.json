{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug tests",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
			"stopOnEntry": false,
			"args": ["${workspaceRoot}/Tests", "--recursive", "--debug", "--no-timeouts"],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": "pretest",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": null
		}
	]
}