#!/usr/local/opt/python3/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'humanfriendly==4.4.1','console_scripts','humanfriendly'
__requires__ = 'humanfriendly==4.4.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('humanfriendly==4.4.1', 'console_scripts', 'humanfriendly')()
    )
