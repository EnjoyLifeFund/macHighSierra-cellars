<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>homebrew.mxcl.chromedriver</string>
  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <false/>
  <key>ProgramArguments</key>
  <array>
    <string>/usr/local/opt/chromedriver/bin/chromedriver</string>
  </array>
  <key>ServiceDescription</key>
  <string>Chrome Driver</string>
  <key>StandardErrorPath</key>
  <string>/usr/local/var/log/chromedriver-error.log</string>
  <key>StandardOutPath</key>
  <string>/usr/local/var/log/chromedriver-output.log</string>
</dict>
</plist>
