#!/usr/local/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'PassportEye==1.0.2','console_scripts','mrz'
__requires__ = 'PassportEye==1.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('PassportEye==1.0.2', 'console_scripts', 'mrz')()
    )
